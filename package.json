{
	"name": "chat.cueup",
	"version": "1.0.0",
	"description": "Chat API originally used for Cueup",
	"main": "server/index.js",
	"repository": "https://github.com/CruelMoney/chat.Cueup.git",
	"author": "Christopher DengsÃ¸",
	"license": "MIT",
	"private": false,
	"engines": {
		"node": ">=8.5.0"
	},
	"scripts": {
		"prestart": "yarn build:server",
		"precommit": "yarn lint && yarn test",
		"prepush": "yarn lint && yarn test",
		"start": "node dist/server/index.js",
		"start:dev": "nodemon",
		"build": "yarn build:server",
		"build:server": "yarn babel --extensions '.ts,.js' server -d dist/server",
		"lint": "node_modules/eslint-watch/bin/esw *.js server config --color",
		"lint:watch": "yarn lint -- --watch",
		"test": "cross-env NODE_ENV=test babel-node mocha --ui bdd --reporter spec --colors --compilers js:babel-core/register ./server/**/*.test.js",
		"test:watch": "yarn test -- --watch",
		"test:unit": "cross-env NODE_ENV=test mocha --require @babel/register 'server/**/unit/**/*.spec.js' --exit"
	},
	"dependencies": {
		"basic-auth": "^2.0.1",
		"bluebird": "^3.5.2",
		"body-parser": "^1.18.3",
		"compression": "^1.7.3",
		"cookie-parser": "^1.4.3",
		"cors": "^2.8.4",
		"createsend-node": "^0.8.2",
		"dotenv": "^4.0.0",
		"expo-server-sdk": "^3.2.0",
		"express": "^4.16.4",
		"express-jwt": "^5.3.1",
		"express-validation": "^1.0.2",
		"express-winston": "^2.4.0",
		"helmet": "^3.14.0",
		"http-status": "^1.2.0",
		"joi": "^13.0.1",
		"jsonwebtoken": "^8.3.0",
		"jwks-rsa": "^1.3.0",
		"method-override": "^2.3.10",
		"mongoose": "^5.3.7",
		"morgan": "^1.9.1",
		"node-fetch": "^1.7.3",
		"socket.io": "^2.1.1",
		"socketio-jwt": "^4.5.0",
		"winston": "^2.4.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.4.4",
		"@babel/core": "^7.4.4",
		"@babel/node": "^7.0.0",
		"@babel/preset-env": "^7.4.4",
		"@babel/preset-typescript": "^7.3.3",
		"@babel/register": "^7.0.0",
		"chai": "^4.2.0",
		"cross-env": "^5.1.0",
		"eslint": "^4.9.0",
		"eslint-config-airbnb-base": "^12.1.0",
		"eslint-plugin-import": "^2.8.0",
		"eslint-watch": "^3.1.3",
		"mocha": "^4.0.1",
		"nodemon": "^1.12.1",
		"sinon": "^7.3.2"
	}
}
