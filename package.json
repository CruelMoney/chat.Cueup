{
  "name": "chat.cueup",
  "version": "1.0.0",
  "description": "Chat API originally used for Cueup",
  "main": "server/index.js",
  "repository": "https://github.com/CruelMoney/chat.Cueup.git",
  "author": "Christopher DengsÃ¸",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=8.5.0"
  },
  "scripts": {
    "prestart": "npm run build:server",
    "precommit": "npm run lint && npm run test",
    "prepush": "npm run lint && npm run test",
    "start": "node dist/server/index.js",
    "dev": "nodemon --exec DEBUG=node* node_modules/babel-cli/bin/babel-node.js server",
    "build": "npm run build:server",
    "build:server": "node_modules/babel-cli/bin/babel.js server --out-dir dist/server --verbose",
    "lint": "node_modules/eslint-watch/bin/esw *.js server config --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "cross-env NODE_ENV=test babel-node node_modules/mocha/bin/_mocha -- --ui bdd --reporter spec --colors --compilers js:babel-core/register ./server/**/*.test.js",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "express-validation": "^1.0.2",
    "express-winston": "^2.4.0",
    "helmet": "^3.9.0",
    "http-status": "^1.0.1",
    "joi": "^13.0.1",
    "jsonwebtoken": "^8.1.0",
    "jwks-rsa": "^1.2.1",
    "method-override": "^2.3.10",
    "mongoose": "^4.12.4",
    "morgan": "^1.9.0",
    "socket.io": "^2.0.4",
    "socketio-jwt": "^4.5.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-latest": "^6.24.1",
    "chai": "^4.1.2",
    "cross-env": "^5.1.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-watch": "^3.1.3",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1"
  },
  "babel": {
    "presets": [
      "latest"
    ],
    "plugins": [
      "add-module-exports",
      "transform-object-assign"
    ]
  }
}
